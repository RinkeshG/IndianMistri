<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.rinkesh.offpay.Dashboard"
    tools:showIn="@layout/app_bar_dashboard">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#FF8A65">



        <!--  First Grid Starts  -->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="20dp"
            android:layout_gravity="center"
            android:gravity="center">


            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:onClick="bookelec"
                android:background="@drawable/whiterectangle">

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center"
                    android:src="@drawable/electrician"
                    android:layout_marginTop="15dp"/>

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="Electrician"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:fontFamily="serif"
                    android:textSize="20dp"/>


            </LinearLayout>


            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:onClick="bookelec"
                android:background="@drawable/whiterectangle"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp">


                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center"
                    android:src="@drawable/plumber"
                    android:layout_marginTop="15dp"/>

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="Plumber"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:fontFamily="serif"
                    android:textSize="20dp"/>

            </LinearLayout>


        </LinearLayout>


        <!--  First Grid Ends -->


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="20dp"
            android:gravity="center">


            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:onClick="bookelec"
                android:background="@drawable/whiterectangle">

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center"
                    android:src="@drawable/carservice"
                    android:layout_marginTop="15dp"/>

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="Car Service"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:fontFamily="serif"
                    android:textSize="20dp"/>


            </LinearLayout>


            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:onClick="bookelec"
                android:background="@drawable/whiterectangle"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp">


                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center"
                    android:src="@drawable/bikeservice"
                    android:layout_marginTop="15dp"/>

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="Bike Services"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:fontFamily="serif"
                    android:textSize="20dp"/>

            </LinearLayout>


        </LinearLayout>


        <!-- Second Grid Ends -->


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="20dp"
            android:gravity="center">


            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:onClick="bookelec"
                android:background="@drawable/whiterectangle">

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center"
                    android:src="@drawable/cleaning"
                    android:layout_marginTop="15dp"/>

                <TextView
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:text="Cleaning"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:fontFamily="serif"
                    android:textSize="20dp"/>


            </LinearLayout>


            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/whiterectangle"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:onClick="bookelec">


                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="70dp"
                    android:layout_gravity="center"
                    android:src="@drawable/dj"
                    android:layout_marginTop="15dp"/>

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="Dj"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:fontFamily="serif"
                    android:textSize="20dp"/>

            </LinearLayout>


        </LinearLayout>

        <!-- Third Grid Ends -->


    </LinearLayout>



</RelativeLayout>
